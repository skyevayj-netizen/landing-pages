  <script>
    function switchLanguage(lang) {
      // ÌòÑÏû¨ Ï†úÌíàÎ™ÖÍ≥º Ïñ∏Ïñ¥ Ï†ïÎ≥¥ Í∞ÄÏ†∏Ïò§Í∏∞
      const productName = 'Product';
      const currentUrl = window.location.href;
      
      // Ïñ∏Ïñ¥Î≥Ñ ÌååÏùºÎ™Ö ÏÉùÏÑ± (Ïòà: AI_Income_english_landing.html)
      const langMap = {
        'en': 'english',
        'es': 'spanish',
        'fr': 'french'
      };
      
      const newFileName = productName.replace(/\s+/g, '_') + '_' + langMap[lang] + '_landing.html';
      
      // Í∞ôÏùÄ ÎîîÎ†âÌÜ†Î¶¨Ïùò Îã§Î•∏ Ïñ∏Ïñ¥ ÌååÏùºÎ°ú Ïù¥Îèô
      const newUrl = currentUrl.replace(/[^/]+\.html$/, newFileName);
      
      // ÌéòÏù¥ÏßÄ Ïù¥Îèô
      window.location.href = newUrl;
    }
  </script>
